{
    "name": "dodyagung/citcuit",
    "description": "A mobile Twitter client alternative. Built with Lumen by Laravel. https://citcuit.in",
    "keywords": ["citcuit", "mobile", "twitter", "client", "alternative", "lumen", "laravel", "secure", "slim", "fast", "lightweight", "no ads", "no database", "indonesia"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0",
        "jublonet/codebird-php": "^3.0",
        "nesbot/carbon": "^1.21"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}

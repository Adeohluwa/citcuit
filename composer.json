{
    "name": "dodyagung/citcuit",
    "description": "A mobile Twitter client from Indonesia. Slim, fast, lightweight, no database, no ads, and of course it's open source.",
    "keywords": ["citcuit", "twitter", "mobile", "client", "indonesia", "slim", "fast", "lightweight", "no database", "no ads", "open source"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0",
        "jublonet/codebird-php": "^2.7",
        "nesbot/carbon": "^1.21"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
